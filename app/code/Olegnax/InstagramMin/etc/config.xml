<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<olegnax_instagram>
			<info>
				<info/>
			</info>
			<general>
				<enabled>1</enabled>
				<use_token>1</use_token>
			</general>
			<oauth>
				<user_id></user_id>
				<username></username>
				<access_token></access_token>
				<expire></expire>
				<authorize><![CDATA[https://olegnax.com/extras/social-connect/instagram/authorize]]></authorize>
			</oauth>
			<scraper>
				<username></username>
			</scraper>
		</olegnax_instagram>
	</default>
</config>
